{
  "lerna": "2.4.0",
  "packages": [
    "detox",
    "detox-cli",
    "detox/test",
    "examples/demo-native-android",
    "examples/demo-native-ios",
    "examples/demo-react-native",
    "examples/demo-react-native-detox-instruments",
    "examples/demo-react-native-jest",
    "examples/demo-plugin",
    "generation",
    "."
  ],
  "version": "16.5.1",
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "@detox/demo-native-android",
        "@detox/demo-native-ios",
        "@detox/demo-plugin",
        "@detox/demo-react-native",
        "@detox/demo-react-native-detox-instruments",
        "@detox/demo-react-native-jest",
        "@detox/generation",
        "@detox/test",
        "."
      ]
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  }
}
