{
  "name": "example",
  "version": "20.0.3-breaking.new-global-lifecycle.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "node scripts/postinstall.js",
    "build:ios": "detox build --configuration ios.sim.debug",
