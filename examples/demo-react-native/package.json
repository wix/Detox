  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.1",
    "detox": "^20.1.3",
    "fs-extra": "^9.1.0",
    "jest": "^29.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.1.3"
  }
